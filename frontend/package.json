{
  "name": "miten",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently --handle-input npm:next-dev npm:watch-graphql",
    "next-dev": "next dev -- -p 4000",
    "build": "next build",
    "export": "next export",
    "next-start": "next start",
    "watch-graphql": "graphql-codegen --watch '../backend/src/**/*.gql' --watch 'components/**/*.ts' --watch 'components/**/*.tsx' --watch 'components/**/*.gql'",
    "generate-types": "graphql-codegen",
    "start:prod": "next build && next start",
    "ts": "tsc --pretty --noEmit -p ./tsconfig.json --skipLibCheck"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@chakra-ui/react": "1.3.2",
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.1.5",
    "apexcharts": "^3.25.0",
    "d3": "^6.5.0",
    "date-fns": "^2.17.0",
    "deepmerge": "^4.2.2",
    "framer-motion": "3.3.0",
    "graphql": "^15.5.0",
    "next": "10.0.6",
    "next-i18next": "^8.1.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "6.15.1",
    "react-icons": "4.2.0",
    "react-spring": "8.0.27",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@types/d3": "^6.3.0",
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.2",
    "@types/styled-components": "5.1.7",
    "babel-plugin-styled-components": "^1.12.0",
    "concurrently": "^5.3.0",
    "typescript": "^4.1.5"
  },
  "license": "GPL-3.0"
}
